{
  "name": "nazg/framework",
  "description": "Http Microframework for Hack",
  "keywords": [
    "hhvm",
    "hack",
    "psr",
    "psr-7",
    "psr-11",
    "psr-15",
    "cache",
    "middleware",
    "dependency injection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=7.0",
    "hhvm": ">=3.25.0",
    "ext-asio": "*",
    "hhvm/hhvm-autoload": "^1.6",
    "hhvm/type-assert": "^3.2.2",
    "hack-psr/psr7-http-message-hhi": "^1.0.0",
    "ytake/hh-container": "^1.0.1",
    "ytake/hh-config-aggregator": "^0.1.7",
    "nazg/heredity": "^0.3.3",
    "nazg/hcache": "^0.1.4",
    "facebook/hack-router": "^0.14",
    "zendframework/zend-diactoros": "^1.7.0",
    "monolog/monolog": "^1.23.0",
    "libreworks/psr3-log-hhi": "^1.0.1",
    "ytake/psr-http-handlers-hhi": "^1.1.0",
    "ytake/psr-container-hhi": "^1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "91carriage/phpunit-hhi": "^5.7",
    "facebook/fbexpect": "^2.0.0",
    "hhvm/hhast": "^1.0.1|>=3.26.0"
  },
  "autoload": {
    "psr-4": {
      "Nazg\\": "src/"
    },
    "files": [
      "src/http_enum.php",
      "src/types.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "NazgTest\\": "tests/"
    },
    "files": [
      "tests/Struct/Shapes.php"
    ]
  },
  "minimum-stability": "stable"
}
